<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.2.5.RELEASE</version>
        <relativePath />
    </parent>

    <groupId>com.sbx</groupId>
    <artifactId>sbx-core-bom</artifactId>
    <packaging>pom</packaging>
    <version>1.0.0-SNAPSHOT</version>
    <description>傻笨象统一版本配置</description>

    <properties>
        <swagger.version>2.9.2</swagger.version>
        <io.swagger.version>1.5.21</io.swagger.version>
        <alibaba.druid.version>1.1.23</alibaba.druid.version>
        <mybatis.plus.version>3.4.0</mybatis.plus.version>
        <dynamic.datasource.version>2.5.8</dynamic.datasource.version>
        <p6spy.version>3.8.7</p6spy.version>
        <alibaba.fastjson.version>1.2.68</alibaba.fastjson.version>
        <protostuff.version>1.6.0</protostuff.version>
        <weixin-java-miniapp.version>3.7.0</weixin-java-miniapp.version>
        <dozer.version>5.5.1</dozer.version>
        <alibaba.easyexcel.version>2.2.6</alibaba.easyexcel.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <!--sbx Start-->
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-mybatis</artifactId>
                <version>${project.version}</version>
                <exclusions>
                    <exclusion>
                        <groupId>com.sbx</groupId>
                        <artifactId>sbx-core-secure</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-model</artifactId>
