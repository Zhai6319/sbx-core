    <distributionManagement>
        <repository>
            <id>rdc-releases</id>
            <url>https://packages.aliyun.com/maven/repository/2005528-release-GzbpjG/</url>
        </repository>
        <snapshotRepository>
            <id>rdc-snapshots</id>
            <url>https://packages.aliyun.com/maven/repository/2005528-snapshot-qgmTh6/</url>
        </snapshotRepository>
    </distributionManagement>

</project>