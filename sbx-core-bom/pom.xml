            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-db</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-launch</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-auth</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-secure</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-starter</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-tool</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-auto</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-oss</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-web</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-swagger</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-cloud</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-log</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-redis</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-sms</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sbx</groupId>
                <artifactId>sbx-core-wx</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!--sbx END-->

            <dependency>
                <groupId>com.alibaba</groupId>
                <artifactId>fastjson</artifactId>
                <version>${alibaba.fastjson.version}</version>
            </dependency>
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-boot-starter</artifactId>
                <version>${mybatis.plus.version}</version>
            </dependency>
            <dependency>
                <groupId>p6spy</groupId>
                <artifactId>p6spy</artifactId>
                <version>${p6spy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.alibaba</groupId>
                <artifactId>druid-spring-boot-starter</artifactId>
                <version>${alibaba.druid.version}</version>
